<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.sykesdev.highwatch" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HighWatch</name>
    <description>
        Highwatch traffic monitoring application for Android/IOS/Browsers.
    </description>
    <author email="ben@sykesdev.ca" href="http://sykesdev.ca">
        Malik Sheharyaar Talhat
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
